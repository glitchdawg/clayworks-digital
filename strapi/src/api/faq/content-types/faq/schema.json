{
    "kind": "collectionType",
    "collectionName": "faqs",
    "info": {
        "singularName": "faq",
        "pluralName": "faqs",
        "displayName": "FAQ",
        "description": "Frequently Asked Questions"
    },
    "options": {
        "draftAndPublish": true
    },
    "pluginOptions": {},
    "attributes": {
        "question": {
            "type": "string",
            "required": true
        },
        "answer": {
            "type": "text",
            "required": true
        },
        "category": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::faq-category.faq-category",
            "inversedBy": "faqs"
        },
        "order": {
            "type": "integer",
            "default": 0
        }
    }
}